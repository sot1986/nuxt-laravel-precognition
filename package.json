{
  "name": "nuxt-laravel-precognition",
  "type": "module",
  "version": "0.0.10",
  "description": "Nuxt module to work with Laravel Precognition and $fetch",
  "author": {
    "name": "sot1986",
    "email": "sot@8dplus.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/sot1986/nuxt-laravel-precognition",
  "repository": {
    "type": "git",
    "url": "https://github.com/sot1986/nuxt-laravel-precognition"
  },
  "keywords": [
    "nuxt",
    "laravel",
    "precognition",
    "ofetch"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release:locally": "npm run lint && npm run test && npm run prepack && npm pack && mv ./nuxt-laravel-*.tgz ../Released",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "defu": "^6.1.2",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.2",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.3",
    "@vitest/coverage-v8": "^0.34.3",
    "autoprefixer": "^10.4.15",
    "changelogen": "^0.5.4",
    "eslint": "^8.47.0",
    "happy-dom": "^10.11.0",
    "nuxt": "^3.6.5",
    "postcss": "^8.4.28",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.22",
    "zod": "^3.22.2"
  }
}