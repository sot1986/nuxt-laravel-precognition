{
  "name": "nuxt-laravel-precognition",
  "type": "module",
  "version": "0.0.12",
  "description": "Nuxt module to work with Laravel Precognition and $fetch",
  "author": {
    "name": "sot1986",
    "email": "sot@8dplus.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/sot1986/nuxt-laravel-precognition",
  "repository": {
    "type": "git",
    "url": "https://github.com/sot1986/nuxt-laravel-precognition"
  },
  "keywords": [
    "nuxt",
    "laravel",
    "precognition",
    "ofetch"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release:locally": "npm run lint && npm run test && npm run prepack && npm pack && mv ./nuxt-laravel-*.tgz ../Released",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.1",
    "defu": "^6.1.4",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.3",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.11.1",
    "@nuxt/test-utils": "^3.12.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.29",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.19",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.11.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.27",
    "zod": "^3.22.4"
  }
}